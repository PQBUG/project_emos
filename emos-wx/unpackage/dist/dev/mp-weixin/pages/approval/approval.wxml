<block wx:if="{{$root.m0}}"><view class="page"><block wx:if="{{processType=='meeting'}}"><view class="meeting"><view class="header">{{title}}</view><view class="attr"><view class="list"><view class="item"><view class="key">日期</view><text class="value">{{date}}</text></view><view class="item"><view class="key">开始时间</view><text class="value">{{start}}</text></view><view class="item"><view class="key">结束时间</view><text class="value">{{end}}</text></view><view class="item"><view class="key">会议类型</view><text class="value">{{typeArray[typeIndex]}}</text></view><block wx:if="{{typeArray[typeIndex]=='线下会议'}}"><view class="item"><view class="key">地点</view><view class="value">{{place}}</view></view></block></view><view><text class="desc">{{desc}}</text></view></view><view class="members"><view class="number">{{"参会者（"+members.length+"人）"}}</view><view class="member"><block wx:for="{{members}}" wx:for-item="one" wx:for-index="__i0__" wx:key="id"><view class="user"><image class="photo" src="{{one.photo}}" mode="widthFix"></image><text class="name">{{one.name}}</text></view></block></view></view><block wx:if="{{taskId!=null}}"><block><button class="btn" type="primary" data-event-opts="{{[['tap',[['approvalMeeting',['同意']]]]]}}" bindtap="__e">同意</button><button class="btn" type="warn" data-event-opts="{{[['tap',[['approvalMeeting',['不同意']]]]]}}" bindtap="__e">不同意</button></block></block></view></block></view></block>